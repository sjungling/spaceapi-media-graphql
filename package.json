{
  "name": "spaceapi-media-graphql",
  "version": "1.0.0",
  "author": "Scott Jungling <scott.jungling@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn generate",
    "prestart": "yarn generate",
    "build": "netlify-lambda build src/graphql --config ./webpack.config.js",
    "start": "npx netlify-lambda -p 8888 serve src/graphql --config ./webpack.config.js",
    "dev": "npx netlify dev",
    "generate": "graphql-codegen --config codegen.yml",
    "format": "prettier -w 'src/**/*.{ts,tsx,graphql,yml,json}' '**/*.md'",
    "check:all": "yarn format"
  },
  "devDependencies": {
    "@apollo/federation": "0.37.1",
    "@babel/helper-get-function-arity": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.20.2",
    "@babel/plugin-transform-object-assign": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@graphql-tools/webpack-loader": "6.7.1",
    "apollo-datasource-rest": "3.7.0",
    "apollo-server-lambda": "3.11.1",
    "babel-cli": "6.26.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "6.4.1",
    "dotenv": "16.0.3",
    "graphql": "16.6.0",
    "netlify-lambda": "2.0.15",
    "netlify-plugin-contextual-env": "0.3.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-node-externals": "3.0.0"
  }
}
